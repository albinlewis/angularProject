{
    "index": "/index.html",
    "assetGroups": [{
            "name": "app",
            "installMode": "prefetch",
            "resources": {
                "files": [

                    "/favicon.ico",
                    "/manifest_app.json",
                    "/index.html",
                    "/*.bundle.css",
                    "/*.bundle.js",
                    "/*.chunk.js"
                ],
                "urls":[
                    "https://use.fontawesome.com/releases/v5.0.13/css/*"
                ]
            }
        },
        {
            "name": "assets",
            "installMode": "lazy",
            "updateMode": "prefetch",
            "resources": {
                "files": [
                    "assets/**"
                ]
            }
        }
    ],
    "dataGroups": [{
        "name": "plants-diseases-api",
        "urls": ["/plants/*", "/diseases/*"],
        "cacheConfig": {
            "strategy": "freshness",
            "maxAge": "3d",
            "timeout": "3s"
        }
    },{
        "name": "history-api",
        "urls": ["/history"],
        "cacheConfig": {
            "strategy": "freshness",
            "maxAge": "3d",
            "timeout": "2s"
        }
    }]
}
